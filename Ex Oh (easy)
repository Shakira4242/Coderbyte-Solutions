function ExOh(str) { 
  var countX = 0;
  var countO = 0;
  str = str.toLowerCase();
  for(var i =0;i<str.length;i++){
  	if(str.charAt(i).search(/[x]/gi) != -1){
    	countX += 1;
    }
    if(str.charAt(i).search(/[o]/gi) != -1){
    	countO += 1;
    }
  }
  if(countX == countO){
  	return true;
  }
  else{
  	return false;
  }
}
   
// keep this function call here 
// to see how to enter arguments in JavaScript scroll down
ExOh(readline());      
